{
  "name": "ng-query",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "c": "cz",
    "build": "nx build ng-query",
    "build:playground": "nx build playground --skip-nx-cache",
    "build:devtools": "nx build devtools",
